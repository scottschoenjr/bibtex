@article{lu_passive_2018,
	title = {Passive acoustic mapping of cavitation using eigenspace-based robust {Capon} beamformer in ultrasound therapy},
	volume = {41},
	issn = {1350-4177},
	url = {http://www.sciencedirect.com/science/article/pii/S1350417717304881},
