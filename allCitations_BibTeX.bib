	pages = {301--308},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\sschoen3\\Zotero\\storage\\WPZQVJPY\\365243.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\sschoen3\\Zotero\\storage\\EAJ3AYIQ\\Walker and Trahey - 1995 - A fundamental limit on delay estimation using part.pdf:application/pdf}
