@article{huang_opening_2016,
	title = {Opening the {Blood}-{Brain} {Barrier} with {MR} {Imaging}–guided {Focused} {Ultrasound}: {Preclinical} {Testing} on a {Trans}–{Human} {Skull} {Porcine} {Model}},
	volume = {282},
